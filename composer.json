{
    "name": "skillpedialms/laravel-h5p",
    "description": "H5P Plugin for laravel TheSkillPedia",
    "type": "library",
    "authors": [{
            "name": "Ram N Sangwan",
            "email": "contact@rnsangwan.com"
        }
      ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "SkillPedia\\LaravelH5p\\": "src/LaravelH5p"
        }
    },
    "require": {
        "php": ">=7.4",
        "laravel/framework": "^8|^9",
        "laravelcollective/html": ">=6.0.0",
        "skillpedialms/h5p-core": "v1.0.0",
        "skillpedialms/h5p-editor": "v1.0.0",
        "guzzlehttp/guzzle": ">=6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0",
        "laravel/legacy-factories": "^1.0.4",
        "guzzlehttp/guzzle": "^7"
     },
    "extra": {
        "laravel": {
            "providers": [
                "SkillPedia\\LaravelH5p\\LaravelH5pServiceProvider"
            ],
            "aliases": {
                "SkillPedia": "SkillPedia\\LaravelH5p\\Facades"
            }
        }
    }
}
